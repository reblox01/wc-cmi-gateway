msgstr "الوضع:"

msgid "Test Mode"
msgstr "وضع الاختبار"

msgid "Live Mode"
msgstr "الوضع المباشر"

msgid "Amount below minimum: %s"
msgstr "المبلغ أقل من الحد الأدنى: %s"

msgid "Amount above maximum: %s"
msgstr "المبلغ أعلى من الحد الأقصى: %s"

